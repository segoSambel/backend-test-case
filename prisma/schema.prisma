generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Member {
    code String @id @db.Char(4)
    name String

    @@map("members")
}

model Book {
    code   String @id @db.VarChar(8)
    title  String
    author String
    stock  Int

    @@map("books")
}
